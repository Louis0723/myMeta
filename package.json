{
  "name": "thewallet",
  "version": "0.4.1",
  "description": "The Offline Wallet for Ethereum, Bitcoin",
  "main": "index.sh",
  "scripts": {
    "start": "webpack-dev-server",
    "install": "./install.sh",
    "build": "rimraf dist && webpack",
    "build.test": "rimraf dist && webpack --config webpack.config.test.js",
    "build.prod": "rimraf dist && webpack --config webpack.config.prod.js",
    "buildWindows": "electron-packager ./docs --platform=win32 --win --out TheWallet --arch=x64 --overwrite --ignore=node_modules",
    "buildMacOS": "electron-packager ./docs --platform=darwin --win --out TheWallet --arch=x64 --overwrite --ignore=node_modules",
    "buildLinux": "electron-packager ./docs --platform=linux --win --out TheWallet --arch=x64 --overwrite --ignore=node_modules"
  },
  "keywords": [
    "thewallet",
    "the wallet",
    "wallet",
    "web wallet",
    "web",
    "cool wallet",
    "cool",
    "brain wallet",
    "brain",
    "offline wallet",
    "offline",
    "bitcoin",
    "btc",
    "ethereum",
    "eth",
    "smart contract",
    "contract",
    "solidity"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/lursun/myMeta.git"
  },
  "author": "Rei-Shan Lu Team - lursun",
  "license": "MIT",
