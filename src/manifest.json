{
  "name": "The Wallet",
  "manifest_version": 2,
  "version": "0.5.0",
  "description": "The Wallet for Ethereum",
  "icons": {
    "16": "img/logo_16.png",
    "48": "img/logo_48.png",
    "128": "img/logo_128.png"
  },
  "default_locale": "en", 
  "background": {
    "scripts": ["background.js"]
  },
  "browser_action": {
    "default_popup": "index.html",
    "default_title": "The Wallet",
    "default_icon": "img/logo_16.png"
  },
  "content_scripts": [{
    "js": ["content.js"],
    "matches": ["<all_urls>"],
    "run_at": "document_start"
  }],
  "permissions": ["storage", "http://*/", "https://*/"]
